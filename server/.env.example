# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/pokemon_finder

# Redis
REDIS_URL=redis://:redis@localhost:6379

# Server
NODE_ENV=development
PORT=3001

# eBay API
EBAY_APP_ID=your_ebay_app_id
EBAY_CERT_ID=your_ebay_cert_id
EBAY_CLIENT_ID=your_ebay_client_id
EBAY_CLIENT_SECRET=your_ebay_client_secret
EBAY_ENVIRONMENT=SANDBOX  # or PRODUCTION

# JustTCG API
JUSTTCG_API_KEY=your_justtcg_api_key

# OpenAI API
OPENAI_API_KEY=your_openai_key
OPENAI_ORGANIZATION=  # optional

# Application Configuration
MAX_LISTINGS_PER_SEARCH=500
WORKER_CONCURRENCY=5
MAX_JOB_ATTEMPTS=3
ENABLE_TELEMETRY=false

# Rate Limiting
EBAY_RATE_LIMIT_PER_SECOND=5
JUSTTCG_RATE_LIMIT_PER_MINUTE=60
OPENAI_RATE_LIMIT_PER_MINUTE=60

# Caching
PRICING_CACHE_TTL_HOURS=24
